clear()
  &:after
    content ''
    display table
    clear both

vendor(prop, args)
  -webkit-{prop} args
  -moz-{prop} args
	{prop} args

transform()
	vendor('transform', arguments)

transition()
	vendor('transition', arguments)

wordbreak()
  vendor('hyphens', auto)
  vendor('word-break', break-all)
  word-break break-word
